<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT FILESYSTEM (MYFILE+)>
<!ELEMENT MYFILE (TITLE,AUTHOR,EMAIL,PageAuthor)>
<!ELEMENT TITLE (#PCDATA)>
<!ELEMENT AUTHOR (#PCDATA)>
<!ATTLIST AUTHOR 
			AUTHORID CDATA #REQUIRED>
<!ELEMENT EMAIL (#PCDATA)>
<!ELEMENT PageAuthor (#PCDATA)>
<!ATTLIST PageAuthor
	name CDATA #IMPLIED
	age CDATA #IMPLIED
	tel CDATA #REQUIRED
	job CDATA #FIXED "PageAuthor"
	favorite CDATA "onLine">